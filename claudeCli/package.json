{
  "name": "claudia",
  "private": true,
  "version": "0.1.65",
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --config vite.config.production.ts",
    "build:debug": "tsc && vite build --mode development",
    "prebuild": "bun run build:executables:current",
    "build:executables": "bun run scripts/fetch-and-build.js --version=1.0.83",
    "build:executables:current": "bun run scripts/fetch-and-build.js current --version=1.0.83",
    "build:executables:linux": "bun run scripts/fetch-and-build.js linux --version=1.0.83",
    "build:executables:macos": "bun run scripts/fetch-and-build.js macos --version=1.0.83",
    "build:executables:windows": "bun run scripts/fetch-and-build.js windows --version=1.0.83",
    "tauri:dev": "tauri dev -c src-tauri/tauri.conf.debug.json",
    "tauri:build": "tauri build -c src-tauri/tauri.conf.release.json",
    "tauri:build:debug": "tauri build --debug -c src-tauri/tauri.conf.debug.json",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "tauri": "tauri"
  },
  "dependencies": {
